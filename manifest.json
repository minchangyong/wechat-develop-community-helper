{
   "background": {
      "scripts": [
         "js/jquery.min.js",
         "background.js"
      ]
   },
   "browser_action": {
      "default_icon": "19.png",
      "default_popup": "popup.html",
      "default_title": "zh_CN"
   },
   "content_scripts": [ {
      "all_frames": true,
      "css": [ "content.css" ],
      "js": [ "js/jquery.min.js", "content.js"],
      "matches": [ "https://developers.weixin.qq.com/community/develop/*" ],
      "run_at": "document_idle"
   } ],
   "description": "微信开发者社区小助手",
   "icons": {
      "128": "128.png",
      "16": "19.png",
      "48": "48.png"
   },
   "manifest_version": 2,
   "name": "微信开发者社区小助手",
   "permissions": [ "tabs", "contextMenus", "storage", "activeTab", "*://developers.weixin.qq.com/*", "clipboardWrite", "webRequest", "webRequestBlocking" ],
   "short_name": "社区小助手",
   "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
   "version": "1.0.8"
}
